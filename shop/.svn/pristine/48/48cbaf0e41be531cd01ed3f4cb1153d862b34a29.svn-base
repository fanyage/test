package com.up.entity;

import java.math.BigDecimal;

public class CommodityEntity {
    private long id;//id
    private String name;//名称
    private BigDecimal price;//价格
    private String photo;//图片
    private String  intro;//介绍
    private String category;//分类
    private String sum;//数量
    private String buycount;//购买次数
    private long shopnameid;//店名id
    private String putaway;//上下架
    private long version;//乐观锁

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public BigDecimal getPrice() {
        return price;
    }

    public void setPrice(BigDecimal price) {
        this.price = price;
    }

    public String getPhoto() {
        return photo;
    }

    public void setPhoto(String photo) {
        this.photo = photo;
    }

    public String getIntro() {
        return intro;
    }

    public void setIntro(String intro) {
        this.intro = intro;
    }

    public String getCategory() {
        return category;
    }

    public void setCategory(String category) {
        this.category = category;
    }

    public String getSum() {
        return sum;
    }

    public void setSum(String sum) {
        this.sum = sum;
    }

    public String getBuycount() {
        return buycount;
    }

    public void setBuycount(String buycount) {
        this.buycount = buycount;
    }

    public long getShopnameid() {
        return shopnameid;
    }

    public void setShopnameid(long shopnameid) {
        this.shopnameid = shopnameid;
    }

    public String getPutaway() {
        return putaway;
    }

    public void setPutaway(String putaway) {
        this.putaway = putaway;
    }

    public long getVersion() {
        return version;
    }

    public void setVersion(long version) {
        this.version = version;
    }

    @Override
    public String toString() {
        return "CommodityEntity{" +
                "id=" + id +
                ", name='" + name + '\'' +
                ", price=" + price +
                ", photo='" + photo + '\'' +
                ", intro='" + intro + '\'' +
                ", category='" + category + '\'' +
                ", sum='" + sum + '\'' +
                ", buycount='" + buycount + '\'' +
                ", shopnameid=" + shopnameid +
                ", putaway='" + putaway + '\'' +
                ", version=" + version +
                '}';
    }
}
